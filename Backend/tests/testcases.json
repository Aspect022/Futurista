        "name": "Normal Predictive Maintenance Analysis",
        "description": "Test normal predictive maintenance workflow for a healthy vehicle",
        "category": "normal_operations",
        "input": {
          "endpoint": "/maintenance/analyze",
          "payload": {
            "vin": "VIN123ABC",
            "customer_id": "CUST001",
            "priority": "MEDIUM",
            "analysis_type": "predictive"
          }
        },
        "expected_output": {
          "status": "completed",
          "overall_confidence": 0.7,
          "processing_time_seconds": 30.0
        },
        "validation_rules": [
          "Response status should be 200",
          "Overall confidence should be > 0.5",
          "Processing time should be < 60 seconds",
          "Should contain results from all worker agents"
        ]
